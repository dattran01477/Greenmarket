<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE tiles-definitions PUBLIC  
      "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
      "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<!-- Base Define -->
	<definition name="base.definition"
		template="/WEB-INF/pages/layouts/defaultlayout.jsp">
		<put-attribute name="header" value="/WEB-INF/pages/layouts/header.jsp" />
		<put-attribute name="body" value="" />
		<put-attribute name="footer" value="/WEB-INF/pages/layouts/footer.jsp" 
			/>
	</definition>


	
	<definition name="Farm" extends="base.definition">
		<!-- <put-attribute name="title" value="Farm Page" /> -->
		<put-attribute name="body"
			value="/WEB-INF/pages/Farm/Index.jsp" />
	</definition>
				
	<!-- Home Page -->
	<definition name="Home" extends="base.definition">
		<!-- <put-attribute name="title" value="Home Page" /> -->
		<put-attribute name="body"
			value="/WEB-INF/pages/Home/Home.jsp" />
	</definition>
	
	<!-- farm Page -->
	<definition name="Farm" extends="base.definition">
		<!-- <put-attribute name="title" value="Home Page" /> -->
		<put-attribute name="body"
			value="/WEB-INF/pages/Farm/Index.jsp" />
	</definition>
	
	
	<!-- Base Account-->
	<definition name="base.account"
		template="/WEB-INF/pages/layouts/account.jsp">
				<put-attribute name="body" value="" />
	</definition>
	<!-- Register -->
	<definition name="Register" extends="base.account">
		<put-attribute name="body"
			value="/WEB-INF/pages/Account/Register.jsp" />
	</definition>
	<definition name="Login" extends="base.account">
		<put-attribute name="body"
			value="/WEB-INF/pages/Account/Login.jsp" />
	</definition>

	
	


</tiles-definitions>